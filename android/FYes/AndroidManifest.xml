<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yesemeinvsuite.android"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="21" />
    
    <!-- 高德start -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <!-- 高德end -->

    <uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.READ_PHONE_STATE" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
	<!-- 以下为可选权限 -->
	<uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT" />
	<uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
	<uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:name="com.fengdu.BaseApplication"
        android:largeHeap="true"
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.yesemeinvsuite.android.MainActivity"/>
        <activity
            android:name="com.yesemeinvsuite.android.WelcomeActivity"
            android:alwaysRetainTaskState="true"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.fengdu.ui.activity.PictureItemActivity"
            android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"/>
        <activity
            android:name="com.fengdu.ui.activity.FeedbackActivity"/>
        <activity
            android:name="com.fengdu.ui.activity.FavoriteActivity"/>
        
        <activity
            android:name="com.fengdu.ui.activity.SearchActivity"/>
        <activity
            android:name="com.fengdu.ui.activity.SettingActivity"/>
        <activity
            android:name="com.fengdu.ui.activity.AboutUsActivity"/>
        
        <service
            android:name="com.fengdu.service.UpgradeService"/>
        <service
            android:name="com.fengdu.service.DownloadService"/>
        
        <!-- 有米ad start-->
        <activity
		    android:name="net.youmi.android.AdBrowser"
		    android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
		    android:theme="@android:style/Theme.Light.NoTitleBar" >
		</activity>
		<service
		    android:name="net.youmi.android.AdService"
		    android:exported="false">
		</service>
		<receiver
		    android:name="net.youmi.android.AdReceiver" >
		    <intent-filter>
		        <action android:name="android.intent.action.PACKAGE_ADDED" />
		        <data android:scheme="package" />
		    </intent-filter>
		</receiver>
      <!-- 有米ad end -->
        
        <!-- 定位start -->
        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="a52db076d24d484ff1c64b2fbae3f6ed" />
        <service android:name="com.amap.api.location.APSService" >
        </service>
        
        <!-- 定位 end -->
        
        <meta-data android:value="56455aede0f55af416004099" android:name="UMENG_APPKEY"/>
		<meta-data android:value="小米" android:name="UMENG_CHANNEL"/>
    </application>

</manifest>
